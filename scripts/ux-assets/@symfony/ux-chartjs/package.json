{
    "name": "@symfony/ux-chartjs",
    "description": "Chart.js integration for Symfony",
    "license": "MIT",
    "version": "1.1.0",
    "type": "module",
    "main": "dist/controller.js",
    "types": "dist/controller.d.ts",
    "scripts": {
        "build": "node ../../../bin/build_package.js .",
        "watch": "node ../../../bin/build_package.js . --watch",
