        "importmap": {
            "@hotwired/stimulus": "^3.0.0",
            "chart.js": "^3.4.1 || ^4.0"
        }
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "chart.js": "^3.4.1 || ^4.0"
    },
    "devDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "chart.js": "^3.4.1 || ^4.0",
        "resize-observer-polyfill": "^1.5.1",
        "vitest-canvas-mock": "^0.3.3"
    }
